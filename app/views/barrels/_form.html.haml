= form_for @barrel do |f|
  - if @barrel.errors.any?
    #error_explanation
      %h2= "#{pluralize(@barrel.errors.count, "error")} prohibited this barrel from being saved:"
      %ul
        - @barrel.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :RFID
    = f.text_field :RFID

    = f.label :name
    = f.text_field :name

    = f.label :type
    = f.text_field :shape




  .actions
    = f.submit 'Save'
